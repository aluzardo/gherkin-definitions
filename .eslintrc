{
  "env": {
      "node": 2
  },
  "globals": {
      "suite": true,
      "test": true
  },
  "rules": {
      "camelcase": 2,
      "curly": 2,
      "eqeqeq": 2,
      "guard-for-in": 2,
      "wrap-iife": 2,
      "no-use-before-define": [2, "nofunc"],
      "new-cap": 2,
      "quotes": [2, "single"],
      "no-trailing-spaces": 2,
      "max-len": 80,
      "no-underscore-dangle": 0,
      "strict": 0,
      "no-shadow": 0
  }
}
